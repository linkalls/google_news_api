{
  "name": "@potetotown/google_news_api",
  "version": "1.0.0",
  "description": "Google News RSS feed to JSON converter",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "author": "poteto",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/linkalls/google_news_api"
  },
  "scripts": {
    "test": "bun test",
